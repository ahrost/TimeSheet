<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
           version="3.0">

    <servlet>
        <servlet-name>getPersonList</servlet-name>
        <servlet-class>controller.GetPersonList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registerPerson</servlet-name>
        <servlet-class>controller.RegisterPerson</servlet-class>
    </servlet>
       <servlet-mapping>
           <servlet-name>registerPerson</servlet-name>
           <url-pattern>/registerPerson.do</url-pattern>
       </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getPersonList</servlet-name>
        <url-pattern>/getPersonList.do</url-pattern>
    </servlet-mapping>


</web-app>
